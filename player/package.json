{
  "name": "DevScape",
  "version": "0.0.1",
  "description": "When DevOps meet Crossy Road <3",
  "scripts": {
    "browser": "opener http://localhost:8080/",
    "dev": "npm run browser && cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "rm -rf build && mkdir build",
    "test": "eslint src tests/**/*.js"
  },
  "keywords": [
    "preact",
    "redux",
    "devops",
    "crossy",
    "road"
  ],
  "author": "katopz",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-minimal": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "core-js": "^2.2.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.10.0",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "localforage": "^1.4.0",
    "opener": "^1.4.1",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "superstatic": "^4.0.1",
    "three": "^0.72.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "decko": "^1.1.3",
    "octokat": "^0.4.17",
    "papergirl": "^0.6.1",
    "preact": "^4.4.0",
    "preact-compat": "^1.6.1",
    "preact-router": "^1.3.0",
    "proptypes": "^0.14.1",
    "randomcolor": "^0.4.3",
    "react": "^0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1"
  }
}
